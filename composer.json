{
  "name": "vsch/laravel-translation-manager",
  "description": "Enhanced Laravel Translation Manager",
  "keywords": [
    "laravel",
    "translations",
    "translator"
  ],
  "homepage": "https://github.com/vsch/laravel-translation-manager",
  "license": "MIT",
  "authors": [
    {
      "name": "Vladimir Schneider",
      "email": "vladimir.schneider@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8",
    "illuminate/support": "^9.0|^10.0",
    "laravelcollective/html": "^6.0",
    "illuminate/translation": "^9.0|^10.0",
    "doctrine/dbal": "^2|^3",
    "symfony/finder": "@stable",
    "ext-zip": "*"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "^10.3"
  },
  "autoload": {
    "classmap": [
      "src/Classes"
    ],
    "files": [
      "src/Support/helpers.php"
    ],
    "psr-4": {
      "Vsch\\TranslationManager\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ],
    "psr-4": {
    }
  },
  "scripts": {
    "post-update-cmd": [
    ]
  },
  "minimum-stability": "stable"
}
